--- /dev/null
+++ b/.github/workflows/windows-test-npm-installation.yml
@@ -0,0 +1,19 @@
+name: '[Windows] Test npm installation'
+
+on:
+  schedule: 
+    - cron: '0 0 * * *'
+  pull_request:
+    branches: [master, release/*]
+
+jobs:
+  build:
+    runs-on: windows-latest
+
+    steps:
+      - uses: actions/checkout@v2
+      - uses: actions/setup-node@v2
+        with:
+          node-version: '14'
+          architecture: 'x64'
+      - run: npm install
